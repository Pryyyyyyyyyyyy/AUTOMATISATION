<!DOCTYPE suite SYSTEM "http://testng.org/testng-1.0.dtd" >
<suite name="Foxtrot Tests" verbose="1">
    <test name="Foxtrot Search Test">
        <parameter name="searchWord" value=""/>
        <parameter name="newWord" value=""/>
        <classes>
            <class name="foxtrot.FoxtrotParameter"/>
            <methods>
                <include name="searchTest"/>
            </methods>
        </classes>
        <methods>
